extends layout


block content
  h1= title

  form(method="POST" style="margin-top: 40px;")
    div.form-group
      input(type="password" id="admin_pass" name="admin_pass" class="form-control")
      label.form-label(for="admin_pass") Admin Pass
    if error
      ul
        li(style="color:red;")= error
    button.btn-submit submit
